head
    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0, user-scalable=no')
    title «Комики и музыканты по своей сути — ворчуны»
    meta(name='description', content='В совместном проекте Samsung Galaxy и Buro 24/7 шоумен Сергей Мезенцев рассказал, как сделать русский стиль классным')
    meta(property='og:image', content='http://api.nahab.info/galaxys8/assets/img/content/1/1.png')
    link(rel='shortcut icon', href='https://api.nahab.info/galaxys8/assets/img/phones.png')
    script(src='https://code.createjs.com/preloadjs-0.6.2.min.js')
    style.
      .bg-placeholder {
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        height: 100%;
        min-height: 600px;
        overflow-x hidden
        transition: opacity 1.2s ease;
        pointer-events none
        width: 100%;
        z-index: 1000;
      }
      .bg-placeholder._loaded {
        opacity: 1;
      }
      .bg-placeholder * {
        pointer-events none
      }

      .bg-placeholder svg {
        height: 100%;
        width: 3700px;
        position: absolute;
        top: 0;
        left: 50%;
        transform: translateX(-50%);
        display: block;
      }
      .screen0 {
        opacity: 0;
        pointer-events: none
      }

      .loader,
      .loader:before,
      .loader:after {
        border-radius: 50%;
        width: 2.5em;
        height: 2.5em;
        -webkit-animation-fill-mode: both;
        animation-fill-mode: both;
        -webkit-animation: load7 1.8s infinite ease-in-out;
        animation: load7 1.8s infinite ease-in-out;
      }

      .loader {
        position: absolute;
        top: 50%;
        left: 50%;
        color: rgba(244,244,244,0.15);
        font-size: 8px;
        position: relative;
        text-indent: -9999em;
        transform: translate(-50%, -50%, 0);
        animation-delay: -0.16s;
        z-index: 1;
      }

      .loader:before,
      .loader:after {
        content: '';
        position: absolute;
        top: 0;
      }

      .loader:before {
        left: -3.5em;
        -webkit-animation-delay: -0.32s;
        animation-delay: -0.32s;
      }

      .loader:after {
        left: 3.5em;
      }

      @-moz-keyframes load7 {
        0%, 80%, 100% {
          box-shadow: 0 2.5em 0 -1.3em;
        }

        40% {
          box-shadow: 0 2.5em 0 0;
        }
      }

      @-webkit-keyframes load7 {
        0%, 80%, 100% {
          box-shadow: 0 2.5em 0 -1.3em;
        }

        40% {
          box-shadow: 0 2.5em 0 0;
        }
      }

      @-o-keyframes load7 {
        0%, 80%, 100% {
          box-shadow: 0 2.5em 0 -1.3em;
        }

        40% {
          box-shadow: 0 2.5em 0 0;
        }
      }

      @keyframes load7 {
        0%, 80%, 100% {
          box-shadow: 0 2.5em 0 -1.3em;
        }

        40% {
          box-shadow: 0 2.5em 0 0;
        }
      }
      @keyframes fadeIn {
        from {
          opacity: 0;
        }

        to {
          opacity: 1;
        }
      }
      .logos {
        background: url('data:image/png;base64,') 50% no-repeat;
        animation-name: fadeIn;
        animation-duration: 1.5s;
        width: 218px;
        height: 58px;
        position: absolute;
        left: 50%;
        margin-left: -78px;
        top: 24px;
        z-index: 1001;
      }

      .logos .buro {
        position: absolute;
        left: 0;
        top: 0;
        height: 58px;
        width: 66px;
      }

      .logos .samsung {
        position: absolute;
        right: 0;
        top: 6px;
        height: 18px;
        width: 116px;
      }
